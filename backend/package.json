    "dotenv": "^16.3.1",
    "express": "^4.22.1",
    "firebase-admin": "^12.0.0",
    "firebase-functions": "^4.6.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.6",
    "firebase-functions-test": "^3.1.0",
    "firebase-tools": "^15.1.0",
